`timescale 1ns/100ps

module tb_dlatch;
	reg tb_clk, tb_d, tb_q;
	wire tb_q_bar;
	
	_dlatch U0_dlatch(.clk(tb_clk), .d(tb_d), .q(tb_q), .q_bar(tb_q_bar));
	
	always #10; tb_clk = ~tb_clk; #10; tb_clk = ~tb_clk;
	
	initial
	begin
		tb_clk = 0;
				tb_d = 0;
		#5 	tb_d = 1;
		#7		tb_d = 0;
		#11	tb_d = 1;
		#3		tb_d = 0;
		#6		tb_d = 1;
		#6		tb_d = 0;
		#10 $stop;
	end
	
endmodule
