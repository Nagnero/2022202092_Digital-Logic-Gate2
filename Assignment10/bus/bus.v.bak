module bus(clk, reset_n, m0_req, m0_wr, m0_address, m0_out, m1_req, m1_wr, m1_address, m1_out, s0_dout, s1_dout,
			m0_grant, m1_grant, m_din, s0_sel, s1_sel, s_address, s_wr, s_din);
