module mx2(y, d0, d1, s);
   input s, d0, d1;
   output y;
   
_inv iv0(.y(sb), .a(s));
_nand2 nd20(.y(w0), .a(d0), .b(sb));
_nand2 nd21(.y(w1), .a(d1), .b(s));
_nand2 nd22(.y(y), .a(w0), .b(w1));   
   
endmodule



module _nand2(y, a, b);
   input a, b;
   output y;
   
   assign y = ~(a & b);

endmodule



module _inv(y, a);
   input a;
   output y;
   
   assign y = ~a;
   
endmodule